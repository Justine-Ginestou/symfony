{% extends 'base.html.twig' %}

{% block title %}Ajouter une catégorie{% endblock %}

{% block body %}
    <h1>Ajout d'une nouvelle catégorie</h1>
    {{ form_start(form) }}
        {{ form_row(form.name) }}
        <button class="btn">{{ button_label|default('Save') }}</button>
    {{ form_end(form) }}
    <a href="{{ path('app_index') }}">
        Retour à l'accueil
    </a>
{% endblock %}

